CREATE TABLE pull_requests (
  id        BIGINT(20) NOT NULL AUTO_INCREMENT,
  prid      VARCHAR(20),
  status    VARCHAR(20),
  PRIMARY KEY (id)
);
CREATE TABLE git_repository (
  id        BIGINT(20) NOT NULL AUTO_INCREMENT,
  status      VARCHAR(20),
  build_state    VARCHAR(20),
  build_color    VARCHAR(20),
  commit_difference    BIGINT(20),
  PRIMARY KEY (id)
);
ENGINE = innodb
DEFAULT CHARSET = utf8;